keywords = {1,contested definitions,debates around concepts of,definitional framework,development,health and wellbeing as,holistic research,introduction,policy,the philosophy of ethics,theoretical framework,well-being,wellbeing,wellbeing traditionally flourished within},
mendeley-tags = {wellbeing},
pages = {116--125},
title = {{Discourse on wellbeing in research and practice}},
url = {http://www.internationaljournalofwellbeing.org/index.php/ijow/article/view/177/315},
volume = {3},
year = {2013}
