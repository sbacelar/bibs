issn = {1522-9602},
journal = {Bulletin of mathematical biology},
keywords = {Algorithms,Evolution, Molecular,Game Theory,Genetics, Population,Models, Genetic,Selection, Genetic,Stochastic Processes},
month = {jul},
number = {5},
pages = {1410--24},
pmid = {18386099},
title = {{Analytical results for individual and group selection of any intensity.}},
