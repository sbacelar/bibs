@article{Acemoglu2013,
abstract = {We study the evolution of a social norm of \cooperation" in a dynamic environment. Each agent lives for two periods and interacts with agents from the previous and next generations via a coordination game. Social norms emerge as patterns of behavior that are stable in part due to agents' interpretations of private information about the past, in uenced by occasional commonly-observed past behaviors. For suciently backward-looking societies, history completely drives equilibrium play, leading to a social norm of high or low cooperation. In more forward-looking societies, there is a pattern of \reversion" whereby play starting with high (low) cooperation reverts toward lower (higher) cooperation. The impact of history can be countered by occasional \prominent" agents, whose actions are visible by all future agents and who can leverage their greater visibility to in uence expectations of future agents and overturn social norms of low cooperation.},
author = {Acemoglu, Daron and Jackson, Matthew O},
doi = {10.2139/ssrn.1839768},
file = {:Users/sergiobacelar/Library/Application Support/Mendeley Desktop/Downloaded/Acemoglu, Jackson - 2013 - History, Expectations, and Leadership in the Evolution of Social Norms.octet-stre:octet-stre},
issn = {1556-5068},
journal = {SSRN Electronic Journal},
keywords = {c72,c73,cooperation,coordination,d7,expectations,history,jel codes,leadership,overlapping generations,p16,repeated games,social norms,z1},
pages = {1--59},
title = {{History, Expectations, and Leadership in Evolution of Social Norms}},
