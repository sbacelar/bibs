journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biotechnology,Biotechnology: legislation & jurisprudence,Biotechnology: methods,Biotechnology: trends,Conservation of Natural Resources,Conservation of Natural Resources: legislation & j,Conservation of Natural Resources: methods,Conservation of Natural Resources: trends,Ecosystem,Environmental Monitoring,Environmental Monitoring: legislation & jurisprude,Environmental Monitoring: methods,Health Promotion,Health Promotion: legislation & jurisprudence,Health Promotion: methods,Health Promotion: trends,Humans,Interdisciplinary Communication,International Cooperation,Publications,Publications: statistics & numerical data,Publications: trends,Social Conditions,Time Factors,World Health},
