issn = {0275-2565},
journal = {American journal of primatology},
keywords = {Age Factors,Animals,Birth Intervals,Computer Simulation,Democratic Republic of the Congo,Demography,Female,Gorilla gorilla,Gorilla gorilla: growth & development,Gorilla gorilla: physiology,Hierarchy, Social,Male,Models, Biological,Mortality,Population Dynamics,Sex Ratio,Uganda},
month = {aug},
number = {4},
pages = {201--23},
pmid = {15300709},
title = {{Simulation of the population dynamics and social structure of the Virunga mountain gorillas.}},
