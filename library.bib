@article{Baseman2015,
abstract = {Humans have a surprising ability to solve hard problems by working collaboratively. Social scientists have long- standing theories that explain collective behavior, but research is ongoing to accurately simulate collective human problem solving in social networks. Efforts to develop and evaluate predictive models for social networks are hindered by the fact that most real social network data is observational, which leaves researchers without any counterfactual ground truth or control groups. Research is further complicated by the dynamic properties of real-world social networks — people are constantly forming new associations, and leaving or entering new social groups. Social computation researchers seek to overcome the limits of observational data by conducting controlled experiments using human subjects. Perhaps the best-known experiments of this kind were conducted by Michael Kearns and his colleagues at the University of Pennsylvania. By imposing a variety of static network structures on individuals in a controlled setting, the researchers provide a unique opportunity to investigate collective problem solving behavior. We use relational and temporal statistical models to simulate the strategies that participants in Kearns et al.'s experiments employed to successfully perform hard computations. Models of individual behavior are composed under a variety of network structures to model collective social network dynamics. Our approach accurately simulates collective behavior, and provides insight into the ways that individuals chose problem solving strategies. Our findings suggest that in collaborative settings, individuals make choices based more on their location within a social network than on intrinsic personal characteristics.},
author = {Baseman, Elisabeth A.},
file = {:Users/sergiobacelar/Documents/papers2Mendeley/CSSSA_2015_submission_8.pdf:pdf},
keywords = {collective behavior,graphical models,social computation},
title = {{Exploring collective behavior in social computation through relational statistical models}},
year = {2015}
}
