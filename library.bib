issn = {23527110},
journal = {SoftwareX},
keywords = {Contingency tables,Correspondence analysis,Interpretation,Package,R},
pages = {26--31},
publisher = {Elsevier B.V.},
title = {{CAinterprTools: An R package to help interpreting Correspondence Analysis' results}},
url = {http://dx.doi.org/10.1016/j.softx.2015.07.001},
volume = {1-2},
year = {2015}
}
