file = {:Users/sergiobacelar/Documents/papers2Mendeley/homo.pdf:pdf},
isbn = {08953309},
issn = {0895-3309},
journal = {Journal of Economic Perspectives. Winter2000},
keywords = {economic forecasting,economics,economists,emotions (psychology),human beings,social scientists},
number = {1},
pages = {133--141},
pmid = {17746758},
title = {{From Homo Economicus to Homo Sapiens.}},
volume = {14},
year = {2000}
}
