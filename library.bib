journal = {Scientific reports},
keywords = {Algorithms,Communicable Diseases,Communicable Diseases: epidemiology,Communicable Diseases: transmission,Computer Simulation,Disease Outbreaks,Disease Outbreaks: statistics & numerical data,Humans,Models, Biological,Population Density,Population Dynamics,Public Health,Public Health: statistics & numerical data,Time Factors,Travel,World Health,World Health: statistics & numerical data},
