isbn = {3905527561},
issn = {22107843},
journal = {Agriculture and Agricultural Science Procedia},
keywords = {Agriculture,Generative Communication,Landscape,Rural Development,Sustainable Communication,Territory,Well-being},
pages = {602--608},
publisher = {Elsevier Srl},
title = {{Well-being, Landscape and Sustainability of Communication}},
