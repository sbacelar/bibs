issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Altruism,Anthropology, Cultural,Archaeology,Biological Evolution,Cooperative Behavior,Cultural Evolution,Female,Genetic Variation,Humans,Male,Microsatellite Repeats,Models, Theoretical,Social Behavior,War},
month = {jun},
