doi = {10.1007/s11205-009-9472-3},
file = {:Users/sergiobacelar/Library/Application Support/Mendeley Desktop/Downloaded/Zhou, Ang, Zhou - 2010 - Weighting and aggregation in composite indicator construction A multiplicative optimization approach.pdf:pdf},
isbn = {03038300 (ISSN)},
