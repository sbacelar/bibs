pmid = {22389765},
title = {{Elementary processes governing the evolution of road networks.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3291042&tool=pmcentrez&rendertype=abstract},
volume = {2},
year = {2012}
}
