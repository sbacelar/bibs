@article{Mathews2012,
abstract = {The first part of this paper discusses why statistical comparison of well-being is not enough.  It considers the criticisms of contemporary analysts, both in terms of the vagaries of cross-individual measurement, and more particularly, of cross-cultural measurement, and discusses how statistical surveys, while necessary, are insufficient. It discusses the problem of surveys both linguistically, given the many subtly different terms in different languages for happiness and well-being, and also culturally, causing the findings of surveys to be less than transparent.  It then turns to a consideration of culture itself, which has become increasingly problematic in anthropology in recent decades, a term that has been shifting in its meanings.  Culture no longer refers simply to “the way of life of a people,” but also to the array of choices individuals make from “the global cultural supermarket”; culture in both these senses needs to be analyzed in terms of how it is introjected into the individual, as recent anthropological theories have been exploring. This new-found complexity of culture does not mean that researchers on subjective well-being should abandon culture as a variable; rather, they should augment statistical surveys of well-being, which are based on the older, conventional conception of culture, with extensive ethnographic interviewing conducted by researchers who understand the language and culture in a given society.  Only on this basis can the cross-cultural study of well-being reach its full potential, the paper argues, a potential uniting different academic disciplines in a common endeavor, that of fully understanding what happiness means and how it can best be attained in the world.},
author = {Mathews, Gordon},
doi = {10.5502/ijw.v2.i4.2},
file = {:Users/sergiobacelar/Documents/papers2Mendeley/132-1106-1-PB - Dan Weijers.pdf:pdf},
issn = {11798602},
journal = {International Journal of Wellbeing},
keywords = {1,1 why the statistical,comparison of wellbeing is,culture,ethnographic research,happiness,not enough,research into happiness and,sophisticated in recent,statistical surveys,the cultural supermarket,well-being,wellbeing,wellbeing has become increasingly},
mendeley-tags = {wellbeing},
pages = {299--312},
title = {{Happiness, culture, and context}},
url = {http://www.internationaljournalofwellbeing.org/index.php/ijow/article/view/132/265},
volume = {2},
year = {2012}
}
