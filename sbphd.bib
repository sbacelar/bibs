@article{belloni2013,
	Abstract = {This study analyses the actuarial characteristics of the
		  Italian pension system throughout its transition from
		  defined benefit (DB) to notional defined contribution (NDC)
		  rules, taking into account expected increasing longevity.
		  Computations rely on ad hoc projected cohort mortality
		  tables based on a limit survival scenario depicted by
		  demographic experts. Most workers retiring in the coming
		  years, whose pension is partly computed according to DB
		  rules, will receive more-than-actuarially fair pensions.
		  However, the generosity of the pension system has been
		  significantly reduced for them by a recent reform that
		  tightened early retirement eligibility requirements.
		  Disability benefits remain (extremely) generous when
		  claimed before age (57) 60. Steady-state NDC pensions, due
		  to dynamic efficiency, are less than actuarially fair. They
		  further deviate from actuarial fairness due to the specific
		  rules, based on historical mortality, adopted by the
		  Italian law for computing and updating benefits while
		  facing increasing longevity. Cohort mortality projections
		  should be used to handle longevity changes in NDC
		  schemes.},
	Author = {Belloni, Michele and Maccheroni, Carlo},
	Doi = {10.1057/gpp.2013.27},
	File = {:Users/sergiobacelar/Documents/papers2Mendeley/belloni2013.pdf:pdf},
	Issn = {1018-5895; 1468-0440},
	Journal = {Geneva Pap. Risk Insur. Pract.},
	Keywords = {actuarial fairness,cohort,contribution,longevity,notional de fi ned,social security},
	Number = {4},
	Pages = {638--674},
	Publisher = {Nature Publishing Group},
	Title = {{Actuarial Fairness When Longevity Increases: An Evaluation of the Italian Pension System}},
	Url = {http://dx.doi.org/10.1057/gpp.2013.27},
	Volume = {38},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1057/gpp.2013.27}}

