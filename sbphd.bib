	Issn = {1091-6490},
	Journal = {Proc. Natl. Acad. Sci. U. S. A.},
	Month = {nov},
	Pages = {1400406111--},
	Pmid = {25404308},
	Title = {{Static network structure can stabilize human cooperation.}},
	Url = {http://www.pnas.org/cgi/content/long/1400406111v1},
	Year = {2014},
