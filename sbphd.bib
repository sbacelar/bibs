@article{walque2005,
	Abstract = {This paper provides a survey on studies that analyze the macroeconomic$\backslash$neffects of intellectual property rights (IPR). The first part of$\backslash$nthis paper introduces different patent policy instruments and reviews$\backslash$ntheir effects on R{\&}D and economic growth. This part also discusses$\backslash$nthe distortionary effects and distributional consequences of IPR$\backslash$nprotection as well as empirical evidence on the effects of patent$\backslash$nrights. Then, the second part considers the international aspects$\backslash$nof IPR protection. In summary, this paper draws the following conclusions$\backslash$nfrom the literature. Firstly, different patent policy instruments$\backslash$nhave different effects on R{\&}D and growth. Secondly, there is empirical$\backslash$nevidence supporting a positive relationship between IPR protection$\backslash$nand innovation, but the evidence is stronger for developed countries$\backslash$nthan for developing countries. Thirdly, the optimal level of IPR$\backslash$nprotection should tradeoff the social benefits of enhanced innovation$\backslash$nagainst the social costs of multiple distortions and income inequality.$\backslash$nFinally, in an open economy, achieving the globally optimal level$\backslash$nof protection requires an international coordination (rather than$\backslash$nthe harmonization) of IPR protection.},
	Author = {Walque, Gregory},
	Date-Added = {2018-04-18 16:52:11 +0000},
