@article{beetsma2012,
	Abstract = {We explore the feasibility of a funded pension system with
		  intergenerational risk sharing when participation in the
		  system is voluntary. Typically, the willingness of the
		  young to participate depends on their belief about the
		  future young's willingness to do so. We characterise
		  equilibria with voluntary participation and show that the
		  likelihood of their existence increases with risk aversion
